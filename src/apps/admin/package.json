{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:next && npm run dev:theme",
    "dev:next": "next dev --turbo -p 3001",
    "dev:theme": "npx tailwindcss -i ./app/theme.css -o ./public/fe-theme.css --watch",
    "build:theme": "npx tailwindcss -i ./app/theme.css -o ./public/fe-theme.css",
    "build": "next build && npm run build:theme",
    "start": "next start -p 3001"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.3.4"
  },
  "devDependencies": {
    "prisma": "^6.11.0",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "@tailwindcss/cli": "^4.1.14"
  }
}
